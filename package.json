{
  "name": "BetterSelf",
  "scripts": {
    "dev": "NODE_ENV=development node server",
    "start": "cross-env NODE_ENV=production node server",
    "build:dev": "rimraf dist && npm run build-dev:client && npm run build-dev:server",
    "build:staging": "rimraf dist && npm run build-staging:client && npm run build-staging:server",
    "build:prod": "rimraf dist && npm run build-prod:client && npm run build-prod:server",
    "build-dev:client": "cross-env NODE_ENV=development webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build-dev:server": "cross-env NODE_ENV=development webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build-staging:client": "cross-env NODE_ENV=staging webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build-staging:server": "cross-env NODE_ENV=staging webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build-prod:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build-prod:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules"
  },
  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "dependencies": {
    "0.0.250": "0.0.249",
    "axios": "^0.17.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.1",
    "es6-promise": "^4.1.1",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "lru-cache": "^4.1.1",
    "node-sass": "^4.9.2",
    "route-cache": "0.4.3",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.16",
    "vue-chat-scroll": "^1.3.3",
    "vue-js-modal": "^1.3.26",
    "vue-meta": "^1.5.5",
    "vue-plain-slider": "^1.0.2",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vue-simple-svg": "^1.3.2",
    "vue-smooth-scroll": "^1.0.13",
    "vue-smoothscroll": "^0.2.0",
    "vue2-smooth-scroll": "^1.0.3",
    "vuelidate": "^0.7.4",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0",
    "wowjs": "^1.1.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-plugin-transform-vue-jsx": "^3.7.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "chokidar": "^1.7.0",
    "css-loader": "^0.28.11",
    "file-loader": "^1.1.5",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.3",
    "style-loader": "^0.21.0",
    "stylus": "^0.54.5",
